{
  "name": "next-boilerplate",
  "version": "0.1.0",
  "main": "server.js",
  "description": "Next Boilerplate",
  "author": "Ilker Guller <me@ilkerguller.com> (http://ilkerguller.com) + TuanCR",
  "license": "MIT",
  "repository": {
    "type": "git"
  },
  "keywords": [
    "next.js",
    "react",
    "graphql",
    "boilerplate",
    "apollo",
    "styled-components",
    "ran",
    "run",
    "toolkit"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint-staged"
    }
  },
  "lint-staged": {
    "**/*.{js,ts}": "npm run lint-staged:js && npm run lint-staged:style",
    "**/*.{jsx,tsx}": "npm run lint-staged:js"
  },
  "scripts": {
    "start": "cross-env NODE_ENV=production node server.js",
    "start:multicore": "cross-env NODE_ENV=production pm2 start server.js -i max --attach",
    "build": "npm run clean-cache && next build",
    "clean-cache": "rimraf -rf ./node_modules/.cache",
    "dev": "npm run lint && npm run clean-cache && nodemon --watch server.js --watch libraries/i18n.js server.js",
    "lint": "npm run lint:js && npm run lint:style",
    "lint:watch": "esw -w --fix components libraries server",
    "lint:js": "eslint --cache --ext .js,.jsx,.ts,.tsx --format=pretty pages components containers libraries server",
    "lint:style": "stylelint './components/**/*.js' './containers/**/*js' './components/**/*.ts' './containers/**/*ts'",
    "lint:fix": "eslint --fix --cache --ext .js,.jsx,.ts,.tsx --format=pretty pages components containers libraries server helper_scripts && npm run lint:style",
    "lint-staged": "lint-staged",
    "lint-staged:js": "eslint --ext .js,.jsx,.ts,.tsx",
    "lint-staged:style": "stylelint './components/**/*.js' './containers/**/*js' './components/**/*.ts' './containers/**/*ts'",
    "analyze": "cross-env ANALYZE_BUILD=true npm run build",
    "setup": "node ./helper_scripts/CL_commands/setup.js && npm run setup:after",
    "setup:clean": "cross-env CLEANSETUP=true node ./helper_scripts/CL_commands/setup.js && npm run setup:after",
    "setup:after": "npm run build",
    "create:page": "node ./helper_scripts/CL_commands/create_page.js",
    "create:route": "node ./helper_scripts/CL_commands/create_route.js",
    "create:container": "node ./helper_scripts/CL_commands/create_container.js",
    "create:component": "node ./helper_scripts/CL_commands/create_component.js",
    "heroku-postbuild": "npm run build",
    "prestart": "npm run build",
    "release": "release",
    "graphql:play": "graphql playground",
    "graphql:update_schema": "graphql get-schema",
    "graphql:see_graph": "graphql voyager",
    "build:static_export": "cross-env STATIC_EXPORT=true npm run build && cross-env STATIC_EXPORT=true next export",
    "zip-for-beanstalk": "npm prune --production && zip -qr ran-boilerplate.zip . -x \"*.zip\""
  },
  "dependencies": {
    "@types/ramda": "^0.26.33",
    "@types/react": "^16.9.2",
    "@types/react-dom": "^16.9.0",
    "@types/react-redux": "^7.1.2",
    "antd": "^3.22.0",
    "apollo-boost": "0.1.23",
    "apollo-cache-inmemory": "^1.6.3",
    "apollo-client": "^2.6.4",
    "apollo-client-preset": "1.0.8",
    "apollo-link-http": "^1.5.16",
    "babel-plugin-import-graphql": "2.6.2",
    "babel-plugin-styled-components": "^1.10.6",
    "chalk": "2.4.1",
    "color": "3.1.0",
    "compression": "1.7.3",
    "cors": "2.8.5",
    "cross-env": "5.2.0",
    "dotenv": "6.2.0",
    "dotenv-webpack": "1.6.0",
    "express": "4.16.4",
    "graphql": "^14.0.2",
    "helmet": "3.15.0",
    "husky": "^1.2.0",
    "ip": "1.1.5",
    "isomorphic-fetch": "2.2.1",
    "js-cookie": "2.2.0",
    "lru-cache": "5.1.1",
    "moment": "2.23.0",
    "next": "~9.0.5",
    "next-cookies": "Sly777/next-cookies#master",
    "next-i18next": "^1.1.0",
    "next-redux-saga": "^4.0.2",
    "next-redux-wrapper": "^3.0.0",
    "next-routes": "1.4.2",
    "offline-plugin": "5.0.6",
    "prop-types": "15.6.2",
    "ramda": "^0.26.1",
    "react": "^16.8.4",
    "react-apollo": "2.3.3",
    "react-dom": "^16.8.4",
    "react-helmet": "5.2.0",
    "react-id-swiper": "^2.4.0",
    "react-perf-devtool": "^3.1.8",
    "react-redux": "^7.1.0",
    "redux": "4.0.1",
    "redux-saga": "^1.0.5",
    "rimraf": "2.6.3",
    "styled-components": "4.1.3",
    "swiper": "^5.2.1"
  },
  "devDependencies": {
    "@babel/plugin-proposal-decorators": "^7.4.4",
    "@babel/plugin-transform-flow-strip-types": "7.2.3",
    "@types/next": "^8.0.6",
    "@types/styled-components": "^4.1.19",
    "@typescript-eslint/eslint-plugin": "^1.4.2",
    "@typescript-eslint/parser": "canary",
    "@zeit/next-css": "^1.0.1",
    "@zeit/next-less": "^1.0.1",
    "babel-eslint": "10.0.1",
    "babel-plugin-module-resolver": "^3.2.0",
    "babel-plugin-transform-remove-console": "^6.9.4",
    "cli-clear": "1.0.4",
    "eslint": "^5.16.0",
    "eslint-config-airbnb": "17.1.0",
    "eslint-config-airbnb-typescript": "^4.0.0",
    "eslint-config-prettier": "^6.1.0",
    "eslint-formatter-pretty": "^2.1.1",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-loader": "2.1.1",
    "eslint-plugin-compat": "^2.6.2",
    "eslint-plugin-graphql": "3.0.1",
    "eslint-plugin-import": "2.14.0",
    "eslint-plugin-jest": "^22.15.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-plugin-react": "7.12.2",
    "eslint-plugin-react-hooks": "^1.6.0",
    "eslint-plugin-unicorn": "^10.0.0",
    "eslint-watch": "4.0.2",
    "figlet": "1.2.1",
    "flow-bin": "0.89.0",
    "graphql-cli": "3.0.4",
    "graphql-cli-voyager": "0.1.3",
    "handlebars": "4.0.12",
    "husky": "1.3.1",
    "inquirer": "6.2.1",
    "less": "^3.10.3",
    "lint-staged": "8.1.0",
    "next-compose-plugins": "^2.2.0",
    "next-fonts": "^0.18.0",
    "next-images": "^1.2.0",
    "ngrok": "3.1.0",
    "nodemon": "^1.19.3",
    "pre-commit": "1.2.2",
    "prettier": "^1.18.2",
    "replace-in-file": "3.4.2",
    "shelljs": "0.8.3",
    "stylelint": "^10.1.0",
    "stylelint-config-recommended": "^2.2.0",
    "stylelint-config-styled-components": "^0.1.1",
    "stylelint-processor-styled-components": "^1.8.0",
    "typescript": "^3.5.3",
    "webpack": "^4.41.0",
    "webpack-bundle-analyzer": "3.0.3"
  },
  "collective": {
    "type": "opencollective",
    "url": "https://opencollective.com/ran"
  }
}
